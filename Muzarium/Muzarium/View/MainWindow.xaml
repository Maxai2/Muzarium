<Window x:Class="Muzarium.View.MainWindow"
								xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
								xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
								xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
								xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
								xmlns:local="clr-namespace:Muzarium.View"
								mc:Ignorable="d"
								Title="MainWindow" Height="750" Width="1000">
	<StackPanel>

		<Border Grid.Row="0" CornerRadius="6" Background="Transparent" BorderThickness="2" BorderBrush="#2D3E50">
			<StackPanel Background="#2D3E50">
				<StackPanel HorizontalAlignment="Left">
					<Image Source="../Resources/MuzariumIcon.PNG" Height="50" Margin="30 0 0 0" Cursor="Hand"/>
				</StackPanel>
				
			</StackPanel>
		</Border>

			<Frame/>

	</StackPanel>
</Window>

